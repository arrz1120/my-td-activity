/*页面加载进度条*/
.wb-fix {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  padding: 0 3%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 99;
  -webkit-box-pack: center;
  box-pack: center;
  -webkit-box-align: center;
  box-align: center;
  display: none;
}
.wb-fix.show {
  display: -webkit-box;
  display: box;
}
.wb-fix.cm-loading {
  background: none;
  z-index: 100;
}
.wb-fix.cm-loading .load {
  background: rgba(0, 0, 0, 0.5);
  width: 70px;
  height: 70px;
  padding: 15px;
  border-radius: 10px;
}

.loading-box{height: 51px;position: absolute;top: 50%;margin-top: -26px;text-align: center;display:-webkit-box;-webkit-box-pack:center; -webkit-box-align:center;width: 100%;z-index:10000;}
.loading-tips{padding:13px 15px;background: rgba(255,255,255,0.95);border-radius: 4px;box-shadow: 1px 2px 10px 0px rgba( 0, 0, 0 ,0.15);}
.loading-tips img{width: 22px;height: 22px;display: inline-block;vertical-align: -6px;-webkit-animation: loading 0.5s linear infinite;}
.loading-tips span{display: inline-block;font-size: 15px;color: #808080;margin-left: 14px;}
@-webkit-keyframes loading {
    from {-webkit-transform:rotate(0deg) translateZ(0); }
    to {-webkit-transform:rotate(360deg) translateZ(0); }
}

/*iscroll css*/
#wrapper {position:absolute; z-index:1;top:5px; bottom:0; left:0;width:100%;background:#f1f3f5;overflow:auto;}
#scroller {position:relative;-webkit-tap-highlight-color:rgba(0,0,0,0);float:left;width:100%;padding:0;}
#scroller ul {position:relative;list-style:none;padding:0;margin:0;width:100%;text-align:left;}
#scroller ul li{ margin-bottom: 15px;}
#scroller li > a {display:block;}

#wrapper #pullDown,#wrapper #pullUp {height:40px;display:-webkit-box;-webkit-box-pack:center;-webkit-box-align:center;}
#wrapper .pullDownIcon{display: block;width: 25px;height: 25px;background: url(/imgs/images/icon/pullDown.png) no-repeat;background-size: 100% 100%;-webkit-transition: all linear 0.2s;transition: all linear 0.2s;}
#wrapper #pullDown .pullDownIcon{-webkit-transform: rotateZ(0);transform: rotateZ(0);}
#wrapper #pullUp .pullDownIcon{-webkit-transform: rotateZ(-180deg);transform: rotateZ(-180deg);}

#wrapper .pulling .pullLoading{display: none;}
#wrapper .loading .pullDownIcon{display: none;}
#wrapper .loading .pullLoading{display: block;width: 25px;height: 25px;position: relative;}
#wrapper .loading .pullLoading .loading-circle{width: 25px;height: 25px;background: url(/imgs/images/icon/loading-circle.png) no-repeat;background-size: 100% 100%;}
#wrapper .loading .pullLoading .loading-logo{width: 21px;height: 21px;background: url(/imgs/images/icon/loading-logo.png);background-size: 100% 100%;position: absolute;top: 2px;left: 2px;-webkit-animation: loading-logo 0.8s linear infinite;animation: loading-logo 0.8s linear infinite;}
@-webkit-keyframes loading-logo{
	0{-webkit-transform: rotateY(0);}
	100%{-webkit-transform: rotateY(360deg);}
}
@keyframes loading-logo{
	0{transform: rotateY(0);}
	100%{transform: rotateY(360deg);}
}