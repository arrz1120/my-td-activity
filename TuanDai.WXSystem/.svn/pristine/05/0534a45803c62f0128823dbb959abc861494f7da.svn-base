		<script type="text/javascript">
			$(".monthItem").click(function() {
				var dayList = $(this).next();
				if(dayList.hasClass('hide')) {
					$(this).find('i').removeClass('rotateZ_90').addClass('rotateZ90');
					dayList.removeClass('hide');
				} else {
					$(this).find('i').removeClass('rotateZ90').addClass('rotateZ_90');
					dayList.addClass('hide');
				}
			})
			
			$(".f30rem").each(function(i,item){
				var len = $(item).html().length;
				if(len>13){
					$(item).css('font-size','0.24rem');
				}
			})
			
			//显示弹框
			function openAlert(obj) {
				$(document).scrollTop(0);
				var parent = obj.parent();
				$("#dateAlert").removeClass('hide').removeClass('moveToBottom').addClass('moveToTop');
				setTimeout(function() {
					$("#bigDiv").addClass('hide');
				}, 300);
			}

			//隐藏弹框
			$("#closeAlert").click(function() {
				$("#dateAlert").removeClass('moveToTop').addClass('moveToBottom');
				$("#bigDiv").removeClass('hide');
				setTimeout(function() {
					$("#dateAlert").addClass('hide');
				}, 300);
			});
			
			$(".dayItem").click(function(){
				openAlert($(this));
			})
		</script>
