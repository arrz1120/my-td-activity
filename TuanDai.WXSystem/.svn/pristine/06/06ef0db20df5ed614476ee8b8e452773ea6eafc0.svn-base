	<script type="text/javascript">
		function bankSelect(){
			var bankAlert =  $("#bankAlert");
			var bankName = $("#bankName");
			var bankImg = $("#bankImg");
			$("#bankSelect,#bankContainer").click(function(){
				$("#bankAlert").show();
			})
			
			bankAlert.find('li').click(function(){
				var imgSrc = $(this).find('img').attr('src');
				var selectedName = $(this).find('p:first').html();
				bankImg.find('img').attr('src',imgSrc);
				bankName.html(selectedName);
				bankImg.show();
				bankAlert.hide();
			})
		}
		bankSelect();
		
	</script>

