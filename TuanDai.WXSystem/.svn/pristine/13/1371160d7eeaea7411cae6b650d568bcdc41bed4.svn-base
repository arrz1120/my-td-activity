.opacity0{opacity: 0;}
.opacity05{opacity: 0.5;}
.animate1 .moveToTop1{-webkit-animation: moveToTop1 0.5s linear forwards;}
.animate1 .fadeOut1{opacity:1;-webkit-animation: fadeOut1 0.5s linear forwards;}
.animate1 .fadeOut2{opacity:1;-webkit-animation: fadeOut1 0.5s 0.5s linear forwards;}
.animate1 .fadeIn1{opacity:0;-webkit-animation: fadeIn1 0.5s 0.8s linear forwards;}

.animate2 .moveToTop2{opacity:0;-webkit-animation: moveToTop2 0.5s 0.5s linear forwards;}
.animate2 .moveToTop3{opacity:0;-webkit-animation: moveToTop3 0.5s 0.7s linear forwards;}
.scaleX{-webkit-transition: all 0.24s linear;}
.scaleX0{-webkit-transform: scaleX(0);}
.scaleX1{-webkit-transform: scaleX(1);}

@-webkit-keyframes moveToTop1{
	0%{-webkit-transform: translateY(0);}
	100%{-webkit-transform: translateY(-135px);}
}
@-webkit-keyframes moveToTop2{
	0%{-webkit-transform: translateY(-55px);opacity: 0;}
	100%{-webkit-transform: translateY(-135px);opacity: 1;}
}
@-webkit-keyframes moveToTop3{
	0%{-webkit-transform: translateY(-55px);opacity: 0;}
	100%{-webkit-transform: translateY(-135px);opacity: 1;}
}
@-webkit-keyframes fadeIn1{
	0%{opacity: 0;}
	100%{opacity: 1;}
}
@-webkit-keyframes fadeOut1{
	0%{opacity: 1;}
	100%{opacity: 0;}
}

