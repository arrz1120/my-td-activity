.ico-sp2{height:15px;width:15px;background-position: 0 -53px;}
.ico-sp3{height:15px;width:15px;background-position: 0 -68px;}
.sp-con{padding: 15px 0;height: 100px;}
.sp-con img{ width: 60px; height: 60px;}
.sp-con .con1{left: 70px;top:15px; width: 60%;}
.sp-con .con1 p{line-height: 170%;}
.sp-con .con2{right: 15px;top:15px;text-align: right;}
.sp-con .con2 a{ border:1px solid #fab600; font-size: 13px; color: #fab600; width: 72px;text-align: center; height: 28px; line-height: 28px;border-radius: 3px;display: inline-block; position: absolute;top: 40px;left: -30px;}
.sp-con .con2 p:first-child{ margin-bottom: 25px;}
.no-update{ padding-top: 77px;}
.no-update img{width: 58.4%;margin: 0 auto;}
.no-update .a-link{padding: 66px 40px 0 40px; }
.no-update .a-link a{width: 100%; height: 40px; line-height: 40px;text-align: center;border-radius: 4px;display: block;background: #fd6040; font-size: 16px; color: #ffffff;}


/*媒体查询*/
@media only screen and (min-device-width : 320px) and (max-device-width : 568px) {
	.task-con{left: 60px;}
	.task-con p:nth-child(2){ font-size: 1rem;}
	}
@media screen and (max-device-height: 480px) {
	.task-con{left: 60px;}
	.task-con p:nth-child(2){ font-size: 1rem;}
	}
@media (device-width:375px) and (-webkit-min-device-pixel-ratio:2){
	.task-con{top:15px;left: 70px;}
	.task-con p:nth-child(2){ font-size: 13px;}
	}
@media (device-width:414px) and (-webkit-min-device-pixel-ratio:3.0){
	.task-con{top:15px;left: 70px;}
	.task-con p:nth-child(2){ font-size: 13px;}
	}

