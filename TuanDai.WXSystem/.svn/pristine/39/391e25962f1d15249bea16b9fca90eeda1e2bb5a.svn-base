body,div,section,ul,li,ol,p,input,span,a,h3{margin: 0;padding: 0;-webkit-transform-style: preserve-3d;-webkit-backface-visibility: hidden; box-sizing:border-box;}
img { border: 0; -ms-interpolation-mode: bicubic; max-width: 100%; vertical-align: middle;}
input{-webkit-border-radius: 0;border-radius: 0; appearance:none;-moz-appearance:none; -webkit-appearance:none;outline: 0;}
a,button,input{-webkit-tap-highlight-color:rgba(255,0,0,0);}
