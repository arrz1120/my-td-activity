		<script type="text/javascript">
			
			function bk_tab(){
				$(".bk_tab_box").each(function(i,item){
					var nav = $(item).find('.bk_tab');
					var con = $(item).find('.bk_tab_con');
					nav.find('a').each(function(j,item){
						$(item).click(function(){
							$(item).siblings().removeClass('cur');
							$(item).addClass('cur');
							con.hide();
							con.eq(j).show();
						})
					})
				})
			}
			bk_tab();
		</script>
