							{day: 5,returnMoney: 1234,repayMoney: 0}, 
							{day: 10,returnMoney: 1234,repayMoney: 5678}, 
							{day: 14,returnMoney: 1234,repayMoney: 5678}, 
							{day: 21,returnMoney: 1234,repayMoney: 5678},
							{day: 26,returnMoney: 1234,repayMoney: 5678}
						]
					]
				
				//调用循环选取还款或回款日函数
				mSwiper.lightDay(data);
				
				//头部初始设置
				var m = (new Date).getMonth() + 1;
				if(m < 10) {
					m = '0' + m;
				}
				$("#nowMon").html(m);
				$("#nowY").html((new Date).getFullYear());
				
				//自定义滑动回调函数，接受activeIndex参数
				function slideChange(activeIndex) {
					var ul = $('.swiper-slide').eq(activeIndex).find('ul');
					var nowMon = ul.attr('month');
					var nowY = ul.attr('year');
					if(nowMon < 10) {
						nowMon = '0' + nowMon;
					}
					$("#nowMon").html(nowMon);
					$("#nowY").html(nowY);
				}
			})
		</script>

