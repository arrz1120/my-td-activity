<script type="text/javascript" src="/scripts/fastclick.js"></script>
<script type="text/javascript">
$(function(){
	var table = $("#table");
	w  =$(".l_table").innerWidth()
//	table.width(w);
	var td = table.find('.bg_td');
	td.each(function(i){
		td.eq(i).css({
			'width':(w-20)/4-9
		});
	});
	$(".l_now").css('width',td.width()*2+5);
	$(".l_now").parent().css('width',td.width()*2+10);
	//滚动
//	setInterval(AutoScroll, 2000);
    function AutoScroll(obj) {
        $("#awardList").find("ul:first").animate({
            marginTop: "-37px"
        }, 500, function () {
            $(this).css({ marginTop: "0px" }).find("li:first").appendTo(this);
        });
    }
})
	
	window.onload = function(){
		$(".l_now").removeClass('l_now_down');
		$(".l_now").on('touchstart',function(){
			$(this).addClass('l_now_down');
		}).on('touchend',function(){
			$(this).removeClass('l_now_down');
		})
	}
</script>

