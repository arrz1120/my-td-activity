	<script type="text/javascript" src="/scripts/jquery.min.js"></script>
	<script type="text/javascript">
		var tab = $(".mx-con").find('.mx-tab');
		var allFrame = tab.find('.arrowFrame');
		var allArrow = tab.find('i');
		tab.each(function(i, item) {
			$(item).click(function(e) {
				e.stopPropagation(); 
				var oframe = $(this).find('.arrowFrame');
				var arrow = $(this).find('i');
				if(oframe.css('display') == 'none') {
					allFrame.hide();
					allArrow.css({
						'-webkit-transform': 'rotateZ(0)',
						'transform': 'rotateZ(0)'
					})
					oframe.show();
					arrow.css({
						'-webkit-transform': 'rotateZ(90deg)',
						'transform': 'rotateZ(90deg)'
					})
				} else {
					oframe.hide();
					arrow.css({
						'-webkit-transform': 'rotateZ(0)',
						'transform': 'rotateZ(0)'
					})
				}
			})
		})
	</script>

