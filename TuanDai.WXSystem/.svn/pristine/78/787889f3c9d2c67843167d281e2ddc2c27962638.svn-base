	<script type="text/javascript" src="/scripts/lib/fastclick-jquery-1.8.3.js"></script>
	<script type="text/javascript" src="/scripts/echarts2.js"></script>
	<script type="text/javascript" src="/scripts/swiper.3.1.7.jquery.min.js"></script>
	<script type="text/javascript">
        $(function() {
            FastClick.attach(document.body);

            function chart(id,xData,yData,formatterType) {
                var myChart = echarts.init(document.getElementById(id));
                option = {
                    tooltip: {
                        show: true,
                        trigger: 'axis',
                        triggerOn: 'none',
                        showContent:true,
                        alwaysShowContent:true,
						//confine:true,
                        backgroundColor: 'rgba( 255, 255, 255, 0.8 )',
                        borderColor:'#d3d7db',
                        borderWidth:1,
                        axisPointer: {
                            lineStyle: {
                                color: '#facf2f',
                            },
                            animation: false
                        },
                        padding: [5, 9, 5, 9],
                        formatter: '{c0}' + formatterType,
                        textStyle: {
                            fontSize: 12,
                            color: '#4d4d4d'
                        }

                    },
                    grid: {
                        left: '0',
                        right: '0',
                        top: '0',
                        bottom: '30',
                        x:0,
                    },
                    calculable: true,
                    xAxis: [{
                        type: 'category',
                        boundaryGap: true,
                        axisTick: {show: false},
                        axisLine: {show: false},
                        splitLine: {
                            show: false,
                            lineStyle: {color: '#f4f4f4'}
                        },
                        axisLabel: {
                            show:true,
                            textStyle: {
                                color: '#8a9199',
                                fontSize: 10
                            },
                            margin: 10
                        },
                        data: xData
                    }],

                    yAxis: [{
                        type: 'value',
                        scale:true,
                        axisTick: {
                            show: false
                        },
                        splitNumber: 2,
                        axisLine: {
                            show: false,
                            lineStyle: {
                                width: 1,
                                color: '#ebeef0'
                            }
                        },
                        axisLabel: {
                            show:false,
                            textStyle: {
                                color: '#808080',
                                fontSize: 10
                            },
                            margin: 0
                        },
                        splitLine: {
                            lineStyle: {color: '#f4f4f4'}
                        },
                    }],
                    series: [{
                        symbol: 'image:///imgs/images/icon/yuan4.png',
                        symbolSize: 11,
                        zlevel: 3,
                        z: 3,
	//					name: '净赚收益',
                        type: 'line',
                        smooth: true,
                        areaStyle: {
                            normal: {
                                color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{
                                    offset: 0,
                                    color: 'rgba(250, 207, 47,0.4)'
                                }, {
                                    offset: 1,
                                    color: 'rgba(255, 255, 255,0.4)'
                                }])
                            }
                        },
                        itemStyle: {
                            normal: {
                                lineStyle: {
                                    color: '#facf2f',
                                    width: '2'
                                }
                            }
                        },
                        data: yData
                    }]
                };

                myChart.setOption(option);
            }
            $(function(){
                chart('c1',['12－31','1－1','1－2','1－3','1－4','1－5'],[10002,31458,22222,13658,8700,6500],'万');
                chart('c2',['2016-09','2016-10','2016-11','2016-12','2017-01'],['200','8700','6500','8700','18000'],'个');
            });

			//排名交互
            function addScale(index){
                var topBox = $(".top-box");
                topBox.removeClass("top-scale");
                topBox.eq(index).addClass('top-scale');
            }

            var mySwiper = new Swiper('#swiper', {
                slidesPerView: 1,
                initialSlide :1,
                spaceBetween: 0,
                speed:300,
                onSlideChangeStart:function(swiper){
                    addScale(swiper.activeIndex);
                }
            });

            $(".top-box").each(function(i,item){
                $(item).click(function(){
                    mySwiper.slideTo(i,500,true);
                })
            })

        });

	</script>
