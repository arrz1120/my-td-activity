.rd-but{ cursor:pointer; display:block;width:100%; background: #ffcc00; height:40px; line-height:40px;border: 0; box-shadow: none; font-size: 17px; color: #fff; text-align:center;-webkit-border-radius: 4px;border-radius: 4px;-moz-background-clip: padding;-webkit-background-clip: padding-box;background-clip: padding-box;}/*通用按钮*/


@media screen and (device-aspect-ratio: 40/71) {
	.rd-mid-con{width: 75%;}
	}

@media (device-width:375px) and (-webkit-min-device-pixel-ratio:2){
	.rd-mid-con{width: 65%;}
	}
@media (device-width:414px) and (-webkit-min-device-pixel-ratio:3.0){
	.rd-mid-con{width: 60%;}
	}
	
/*账号管理*/	
.zhgl-top{
	padding-top: 8px;
	background: #fcfcfc;
}	

.zhgl-top .top-p1 span{
	color: #fab700;
}
.zhgl-top .top-p2{
	margin-top: 2px;
	padding-bottom: 6px;
}

.zhgl-item{
	background: #fff;
}

.zhgl-item .top-p1{
	line-height: 45px;
}

.jiebang{
	color: #fab600;
	font-size: 13px;
	display: block;
	line-height: 14px;
	position: absolute;
	right: 15px;
	bottom: 15px;
}
.zhgl-top .jiebang{
	bottom: 10px;
}
.jiebang:after{
	content: '';
	display: inline-block;
	background: url(/imgs/images/icon/arrow_right_orange.png) no-repeat;
	background-size: 100% 100%;
	width: 8px;
	height: 14px;
	margin-left: 11px;
	vertical-align: top;
}

.zhgl-bottom p{line-height: 44px;color: #1fcafb;font-size: 17px}
.zhgl-bottom p:before{
	content: '';
	display: inline-block;
	background: url(/imgs/images/icon/ico_add2.png) no-repeat;
	background-size: 100% 100%;
	width: 18px;
	height: 18px;
	margin-right: 5px;
	vertical-align: m;
}

/*绑定账户*/
/*.inp-box{margin-top: 8px;margin-bottom: 42px;}*/
.inp-box{padding-left: 87px;position: relative;height: 45px;}
.inp-box span{position: absolute;left: 13px;top: 0;display: block;height: 45px;line-height: 45px;}
.inp-box input{border: 0;outline: none;font-size: 17px;height: 45px;width: 60%;}
.inp-err{height: 30px;line-height: 30px;position: absolute;top: 44px;left: 15px;color: #fd6040;font-size: 12px;}
.mt36{margin-top: 36px;}
.sendCode{width: 32%;height: 45px;line-height: 45px;color: #f3b800;font-size: 13px;text-align: right;position: absolute;right: 0;top: 0;padding-right: 15px;}

/*未绑定账户*/
.ico-bind{width: 160px;height: 49px;background: url(/imgs/images/icon/ico_bind.png) no-repeat;background-size: 100% 100%;margin: 40px auto 0;}
