.typeBg img{width: 30px;}

.alert-show{-webkit-animation: aniShow 0.4s cubic-bezier(0.7,0,0.3,1) both;}
.alert-hide{-webkit-animation: aniHide 0.4s cubic-bezier(0.7,0,0.3,1) both;}

@-webkit-keyframes alert-show{
	0%{-webkit-transform: translate3d(0,-200px,0);opacity: 0;}
	100%{-webkit-transform: translate3d(0,0,0);opacity: 1;}
	}
@-webkit-keyframes alert-hide{
	0%{-webkit-transform: translate3d(0,0,0);opacity: 1;}
	100%{-webkit-transform: translate3d(0,200px,0);opacity: 0;}
	}



.qiandao-box{border: 1px solid #e3e5e6;padding: 15px;-webkit-border-radius: 5px; background: #fff;}
.ico_arrow_r3{width: 7px;height: 13px;background: url(/imgs/images/ico_arrow_r3.png) no-repeat;background-size: 100% 100%;right: 15px;top: 15px;}
.alert-con{width: 88%;border-radius: 8px;}
.inp-box{border: 1px solid #ffdc5d;-webkit-border-radius: 5px; height: 40px; line-height: 40px;}
.inp-box input{border: 0;width: 100%; height: 36px; line-height: 36px;-webkit-border-radius: 5px; font-size: 15px;text-indent: 10px; color: #333333;}
.inp-box input::-webkit-input-placeholder { color: #ccc; }
.eye-but{position: absolute;right: 0;top: -1px;width: 40px;height: 40px;}
.eye-close{background: url("/imgs/member/ico-hide.png");height:16px;width:16px;background-size: 16px 16px;}
.eye-open{background: url("/imgs/member/ico-display.png");height:16px;width:16px;background-size: 16px 16px;}

.but-box div{ height: 44px; line-height: 44px;}

.moveFromTop{-webkit-animation: moveFromTop .3s ease-out forwards ;}
.moveToTop{-webkit-animation: moveToTop .3s ease-out forwards ;}

@-webkit-keyframes moveToTop{
	0%{-webkit-transform: translate3d(0,0,0);}
	100%{-webkit-transform: translate3d(0,100%,0);}
	}

@-webkit-keyframes moveFromTop{
	0%{-webkit-transform: translate3d(0,100%,0);}
	100%{-webkit-transform: translate3d(0,0,0);}
	}

/*媒体查询*/
@media screen and (device-aspect-ratio: 40/71) {
	.super-m-slide .swiper-slide .f20px{margin: 15px 18% 0;height: 35px;}
	.super-pagination { margin-top: 10px; }
	.buy-box h3{padding: 10px 0;}
	.buy-box .box-flex1 .lf span{display: block;width: 20px; height: 29px; }
	.buy-box .box-flex1 .m1{background: url("/imgs/member/1months.png") center no-repeat;background-size: 20px 29px;}
	.buy-box .box-flex1 .m6{background: url("/imgs/member/6months.png") center no-repeat;background-size: 20px 29px;}
	.buy-box .box-flex1 .m12{background: url("/imgs/member/12months.png") center no-repeat;background-size: 20px 29px;}
	.sup-m-top .lf .name{width: 100px;}
	.bb-e8eaeb.bg-fafbfc{ padding-left: 10px; padding-right: 10px;}
	.sup-m-top .avatar{ margin-right: 5px;}
	.sup-m-top .f17px{ font-size: 15px!important;}
	.sup-m-top .f13px{ font-size: 11px!important;}
	.sup-m-top .f10px{ font-size: 9px!important;}
	.buy-box .box-flex1 .lf{padding: 7px 4px 7px 4px; }
	.buy-box .box-flex1 .rf{padding: 6px 6px 6px 0;}
	.buy-box .box-flex1 .rf p:first-child{ font-size: 15px; color: #4d4d4d;}
	.buy-box .box-flex1 .rf p:last-child{ font-size: 11px; color: #999999;}
	}

@media (device-width:375px) and (-webkit-min-device-pixel-ratio:2){
	.buy-box .box-flex1 .rf p:first-child{ font-size: 17px;}
	}
@media (device-width:414px) and (-webkit-min-device-pixel-ratio:3.0){
	.buy-box .box-flex1 .rf p:first-child{ font-size: 17px;}
	}

