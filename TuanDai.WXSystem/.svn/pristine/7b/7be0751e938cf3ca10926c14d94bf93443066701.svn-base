html,body,h1,h2,h3,p,span,a,div,ol,ul,li,dl,dt,dd,table,tbody,tfoot,thead,tr,th,td,input,textarea, form,article,aside,details,figcaption,figure,footer,header,hgroup,nav,section { margin: 0; padding: 0; font-family: 'Microsoft Yahei', Arial, Verdana, Helvetica, sans-serif; -webkit-tap-highlight-color: rgba(0,0,0,0); color: #212121;box-sizing: border-box;font-size: 12px;line-height: initial;}

.bt-ffffff,.bb-ffffff,.bl-ffffff,.br-ffffff,.bb-e8eaeb,.bb-ebebeb,.bt-fafbfc{position: relative;}
.bt-ffffff:before{content: "";position: absolute;left: 0;top: 0;right: 0;border-bottom: 1px solid rgba(255,255,255,0.4);transform: scaleY(.5);transform-origin: 0 0;}
.bt-fafbfc:before{content: "";position: absolute;left: 0;top: 0;right: 0;border-bottom: 1px solid #fafbfc;transform: scaleY(.5);transform-origin: 0 0;}
.bb-ffffff:after{content: "";position: absolute;left: 0;bottom: 0;right: 0;border-bottom: 1px solid rgba(255,255,255,0.4);transform: scaleY(.5);transform-origin: 0 100%;}
.bb-e8eaeb:after{content: "";position: absolute;left: 0;bottom: 0;right: 0;border-bottom: 1px solid #e8eaeb;transform: scaleY(.5);transform-origin: 0 100%;}
.bb-ebebeb:after{content: "";position: absolute;left: 0;bottom: 0;right: 0;border-bottom: 1px solid #ebebeb;transform: scaleY(.5);transform-origin: 0 100%;}
.bl-ffffff:before{content: "";position: absolute;top: 0;bottom: 0;left: 0;border-left: 1px solid rgba(255,255,255,0.4);transform: scaleX(.5);transform-origin: 0 0;}
.br-ffffff:after{content: "";position: absolute;top: 0;bottom: 0;right:0;border-right: 1px solid rgba(255,255,255,0.4);transform: scaleX(.5);transform-origin: 0 0;}
.bb-dashed-e6e6e6{border-bottom: 1px dashed #e6e6e6;}


