.headerMain{ height: 48px; width: 100%; position: relative; z-index: 500;}
.headerMain .none{ height: 48px; width: 100%; position: absolute; top: 0; left: 0; z-index: 10;}
.header{ width: 100%; height: 48px;padding:0 10px;background-color: #fff; border-bottom: 1px solid #c2c2c2; position: fixed; top: -4; 
left: 3; 
z-index: 50;
}/*position: fixed; */
.header .back{width: auto; height: 48px; line-height: 48px; color: #000; font-size: 1.3rem; position: absolute; top: 0; left: 15px; z-index: 100; cursor:pointer;}
.header .back:before{content: " "; display: inline-block; width: 10px; height: 18px; vertical-align: middle;background: url(/imgs/images/back_Arrow.png) no-repeat; background-size: 10px 18px; vertical-align: middle; margin-right: 5px;margin-top: -3px;}
.header .title{ text-align: center; width: auto; height: 48px; line-height: 48px;color: #000; font-size: 1.4rem; margin: 0 auto;}
.header .title select{ width:80px; font-size: 1.4rem; border: 0; border-image:url(/imgs/images/select_arrow.png); outline: none; background: url(/imgs/images/select_arrow.png) right center no-repeat; background-size: 13px 7px;}
.header .title .selectBox{ width: 106px; height: 48px; margin: 0 auto; position: relative; z-index: 500;}
.header .title .selectBox .ico-arrow-search{ display: block; width: 13px; height: 7px; background: url(/imgs/images/select_arrow.png) right center no-repeat; background-size: 13px 7px; position: absolute; right: 0; top: 50%; margin-top: -3px;transition:all 0.3s ease-out; -webkit-transition:all 0.3s ease-out; }
.header .title .selectBox .ico-arrow-search.hover{-webkit-transform: rotate(-180deg);transform: rotate(-180deg);}
.header .title .selectBox .selectText{ height: 48px; line-height: 48px;font-size: 1.4rem; color: #000;}
.header .title .selectBox .selectList{ display: none; width: 120px; height: auto; background-color: #fff; border: 1px solid #c2c2c2; border-top: 0; padding-bottom: 10px; position: absolute; top: 47px; left: -7px;}
.header .title .selectBox .selectList li{ height: 32px; line-height: 32px; color: #333; font-size: 1.2rem;}
.header .title .selectBox .selectList li:hover{ background-color: #fbfbf9;}

.header .text{width: auto; height: 48px; line-height: 48px;color: #000; font-size: 1.3rem; position: absolute; top: 0; right: 15px; z-index: 100;}
.header .text a{color: #000; font-size: 1.3rem; line-height: 48px;}
