/**投资列表*****************************/
.contBox{ padding: 15px 80px 10px 15px; min-height: 100px; margin-bottom: 2px; position: relative; z-index: 20;}
.contBox .contLeft{ width: 100%; height: auto;}
.contBox .contLeft .c-bd{display: -moz-box;display: -webkit-box;display:box; margin-top: 5px;}
.contBox .contLeft .c-bd div{-webkit-box-flex: 1;-ms-flex: 1;box-flex: 1;}
.contBox .contRight{ width: 80px; height: 80px; position: absolute; top: 20px; right: 20px;}
.Itip{ height: auto; padding: 0 0 2px 15px;border-top: 1px solid #d9d9d9; background-color: #fffdf5;}
.Itip span{ display: inline-block; width: auto; line-height: 18px; color: #ababab; font-size: 1rem; padding-right: 20px;}
.Itip span i{ display: inline-block; width: 6px; height: 6px; border-radius: 50%;-webkit-border-radius: 50%; background-color: #40d791; vertical-align: middle; margin-right: 5px; margin-top: -1px;}
.Itip span:nth-child(2) i{ background-color: #25cdf6;}
/*环形统计*/
.circle {width: 60px;height: 60px;position: absolute;right:0; top:5px;border-radius: 50%;-webkit-border-radius: 50%;background-color: #fd5f3f;background-image: url(data:image/svg+xml;base64,);
background-image: -webkit-linear-gradient(bottom, #fbc72f 0%, #fc6b4e 100%);
background-image: linear-gradient(bottom, #fbc72f 0%, #fc6b4e 100%);}
.pie_left, .pie_right {width:60px;height:60px;position: absolute;top: 0;left: 0;}
.left, .right {width:60px;height:60px;background:#ebebeb;border-radius: 50%;-webkit-border-radius: 50%;position: absolute;top: 0;left: 0;}
.pie_right, .right {clip:rect(0,auto,auto,30px);}
.pie_left, .left {clip:rect(0,30px,auto,0);}
.circle .mask{width: 52px;height: 52px;border-radius: 50%;-webkit-border-radius: 50%;left: 4px;top: 4px;background: #FFF;position: absolute;text-align: center;line-height: 52px;font-size: 1.4rem;color: #fd6040;}
.circle .mask span{font-size: 1.4rem;color: #fd6040;}
.finish{ background-color: #93d94e!important; background-image: none!important;}
.finish .mask,.finish .mask span{ color: #93d94e!important;}

/**遮罩层***************/
.maskLayer{ height: 100%; width: 100%; background: rgba(0,0,0,.5); position: fixed; top: 0; left: 0; z-index: 1000;}

