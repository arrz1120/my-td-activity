/***自动投标********************************************/
.autoBox{ margin-top: 15px;}
.autoContList{border-bottom: 1px solid #ccc;}
.autoContList:last-child{ border-bottom: 0;}
.autoBox .timeBox{ float: left; width: 40%;}
.autoBox .states{ float: right; width: 20%; margin-top: 10px;}
.autoBox .infoBox{ display: none; padding: 20px 15px; background-color: #fbfbf9; position: relative; z-index: 50;}
.autoBox .infoBox a{ display: block; width: 100%; height: 100%; background: url('/imgs/images/ico_arrow_r.png') center right no-repeat; background-size: 10px 17px;}
.autoBox .infoBox .w-50{ width: 50%;}
.autoBox .arrowDown{ display: block; width: 16px; height: 16px; background-color: #fff;transform:rotate(45deg);-webkit-transform:rotate(45deg); position: absolute; top: -8px; left: 50%; margin-left: -8px;}

/***修改自动投标设置********************************************/
.amendBox{ padding-left: 80px; border-bottom: 1px solid #d9d9d9; padding-bottom: 10px; padding-top: 10px; position: relative; z-index: 10;}
.amendBox:last-child{ border-bottom: 0;}
.amendBox .leftBox{ width: 80px;color: #212121;height:28px; line-height:150%;font-size: 1.4rem; position: absolute; top: 11px; left: 0;}
.amendBox .rightBox{width: 100%;height: 28px;line-height: 150%;font-size: 1.4rem;border: 0;background: none;display: -moz-box;display: -webkit-box;display:box;}
.amendBox .rightBox .contBox{-webkit-box-flex: 1;box-flex: 1;font-size: 1.4rem;color: #212121; min-height:28px!important;height: 28px!important;line-height: 150%; padding: 0!important; padding-top: 1px;}
.amendBox .rightBox .inputBox{-webkit-box-flex: 1;box-flex: 1;font-size: 1.4rem;color: #212121; min-height:28px!important;height: 28px!important;line-height: 150%;padding-top: 1px; padding-right: 70px; position: relative; z-index: 50;}
.amendBox .rightBox .inputBox:last-child{ padding-left: 2px;}
.amendBox .rightBox .inputBox .unit{ display: block; width: 70px; text-align: center; height: 28px; line-height: 28px; color: #212121; font-size: 1.4rem; position: absolute; top: 0; right: 0;}

.amendBox .rightBox .select{border: 0; border-image:url('/imgs/images/ico_arrow_r.png');  outline: none;display:block;width: 100%;height:100%;color: #545454;font-size: 1.4rem;background: url('/imgs/images/ico_arrow_r.png') center right no-repeat; background-size: 10px 17px;}
.amendBox .rightBox .ipt{ width: 100%; height: 100%; border: 0; background: none;font-size: 1.4rem;color: #212121; margin-top: -1px;}
.amendBox .rightBox .date{ width: 100%; border: 0; background: none;font-size: 1.4rem;color: #212121;background: url('/imgs/images/ico_arrow_r.png') center right no-repeat; background-size: 10px 17px;}
.amendBox .rightBox .date:last-child{}
/***自动投标规则*****************************************/
.rulesBox{ padding: 0 15px 0 15px;}
.rulesBox p{ font-size: 1.5rem; margin:16px 0 16px 0; line-height: 24px; }
/***还款方式/投标类型*****************************************/
#TypeMain{bottom:-512px;}
#wayMain{bottom:-430px;}
.automaticwayBox{width:100%; background-color: #fff; padding-top: 5px;position: fixed; left: 0;  z-index: 1010;padding-bottom: 10px;}
.automaticwayBox li{ border-bottom: 1px solid #ccc;padding-left: 15px; padding-right: 15px; }
.automaticwayBox li span{ height: 60px; line-height: 60px;font-size: 1.4rem;display: inline-block;vertical-align: middle;}
.automatic_but{bottom:0px; padding:0 15px 0 15px; width: 100%;background:#fbfbf9;}
.automatic_but span{font-size: 1.4rem;height: 60px; line-height: 57px; display: inline-block;vertical-align: middle; margin-right: 10px;}
.automatic_but .btn_determine{width:40%;display: inline-block;float: right;margin-top:9px; }

.auto-hint p .rule-but:before{ content: " "; display: inline-block; width: 16px; height: 12px; margin-right: 5px; background: url(/imgs/images/ico-qian.png); background-size: 16px 12px; vertical-align: middle; margin-top: -3px;}

.IDtip{ height: auto; padding: 5px 0 5px 10px;background-color: #fffdf5;}
.IDtip span{ display: inline-block; width: auto; line-height: 18px; color: #ababab; font-size: 1rem; padding-right: 20px;}
.IDtip span i{ display: inline-block; width: 8px; height: 8px; border-radius: 50%;-webkit-border-radius: 50%; background-color: #FFCF1F; vertical-align: middle; margin-right: 5px; margin-top: -1px;}
  

/*自动投标  V2.3.7*/
.showDetail{height: 27px;}
.showDetail i{display: block;width: 14px;height: 8px;background: url(/imgs/images/icon/ico-arrow-d-d1d1d.png) no-repeat;background-size: 100% 100%;}
.autoDetail ul{padding-top: 6px;}
.autoDetail li{font-size: 11px;color: #000;position: relative;padding-left: 73px;padding-right: 15px;line-height: 18px;padding-top: 7px;}
.autoDetail li span{font-size: 11px;color: #808080;position:absolute;left: 0;top: 7px;line-height: 18px;}
.rotateZ180{-webkit-transform: rotateZ(180deg);}

/*自动投标  V2.5.0*/
input{border: 0;outline: 0;}
input::-webkit-inner-spin-button {
     -webkit-appearance: none;
}
input::-webkit-outer-spin-button {
   -webkit-appearance: none;
}
.tabBox{padding: 14px 0;}
.tabItem{font-size: 17px;height: 17px;line-height: 17px;color: #999999;text-align: center;}
.tabItem.act{color: #fab600;}
.selectBox ul li{padding-right: 15px;padding-bottom: 15px;float: left;}
.selectBox ul li div{width: 100%;height: 30px;line-height: 30px;border: 1px solid #e6e6e6;color: #808080;border-radius: 2px;text-align: center;font-size: 15px;}
.selectBox ul li div.selected{background: #fffdf7;border: 1px solid #ffe6c5;color: #694514;}
.selectBox1 ul li{width: 50%;}
.selectBox2 ul li{width: 33.33%;}
.inputBox{padding: 0 30px 0 23.3%;height: 45px;}
.inputBox input{height: 45px;width: 100%;font-size: 15px;}
.inputBox span{position: absolute;display: block;height: 45px;line-height: 45px;top: 0;font-size: 15px;}
.inputBox .ib_span1{left: 0;width: 23.3%;}
.inputBox .ib_span2{right: 0;width: 30px;}
.checkBox i{display: inline-block;width: 14px;height: 14px;border: 1px solid #cccccc;border-radius: 2px;background: #fff;border-radius: 2px;vertical-align: -2px;margin-right: 10px;}
.checkBox i.checked{background: #ffcf1f url(/imgs/images/icon/checked.png) center center no-repeat;background-size: 10px 6px;border: 1px solid #ffcf1f;}
.checkBox span:nth-child(2){margin-left: 40px;}
.checkBox span:nth-child(3){margin-left: 30px;}
.dateBox{padding: 11px 15px 22px 0;}
.dateBox input{width: 100%;height: 40px;border: 1px solid #e6e6e6;border-radius: 4px;padding-left: 15px;font-size: 15px;background: #fff;-webkit-appearance: none;}
.dateBox .ico-arrow-rl{right: 30px;}
input[type="date"]:before{
    content: attr(placeholder);
    color:#ccc;
}
.btnWrap{position: absolute;bottom: 0;left: 0;width: 100%;background: #f1f3f5;z-index: 2;}

.ico-edit{display:inline-block;padding:0 20px;width: 15px;height: 17px;background: url(/imgs/images/ico-edit.png) center center no-repeat;background-size: 15px 17px;}

.ico_arrow_r3{width: 7px;height: 13px;background: url(/imgs/images/ico_arrow_r3.png) no-repeat;background-size: 100% 100%;right: 15px;top: 15px;}
.alert-con{width: 86%;border-radius: 8px;}
.alert-income-inp input{width: 70px;border-bottom: 1px solid #d1d1d1;text-align: center;font-size: 23px;}
.alert-income-inp span{margin: 0 25px;}
.alert-input span{display: inline-block;}
.alert-input .inp{width: 170px;height: 40px;border: 1px solid #e6e6e6;border-radius: 5px;position: relative;margin: 0 auto;}
.alert-input .inp span{display: block;height: 40px;line-height: 40px;position: absolute;top: 0;left: -30px;}
.alert-input .inp input{padding-left: 15px;font-size: 15px;width: 109px;height: 38px;position: absolute;left: 0;top: 0;border-radius: 5px;}
.alert-input .inp i.i_month{display: block;width: 30px;height: 38px;line-height: 38px;text-align: center;position: absolute;top: 0;right: 30px;background: #f5f5f5;color: #808080;font-size: 15px;}
.alert-input .inp i.i_day{display: block;width: 30px;height: 38px;line-height: 38px;text-align: center;position: absolute;top: 0;right: 0;background: #f5f5f5;color: #808080;font-size: 15px;border-radius: 0 5px 5px 0;}
.alert-input .inp i.cur{background: #ffcf1f;color: #fff;}

/*设置成功*/
.suc_con{width: 100%;height: 100%;position: fixed;top: 0;left: 0;}
.suc_con img{width: 110px;height: 110px;}
.suc_con p{font-size: 17px;padding-top: 26px;}

/*自动投标  V2.5*/
.ico-arrow-d-ccc{display: block; background: url("/imgs/images/icon/ico-arrow-d-ccc.png") center no-repeat;width: 8px;height: 15px;background-size: 8px 15px;right: 15px;bottom: 30px;z-index: 2;}
.ranking{height: 44px;line-height: 44px;}
.auto-item{ margin-bottom: 10px;}
.auto-item .top-con .lf{ height: 45px; line-height: 45px; font-size: 15px; color: #333333;}
.autoList{-webkit-box-orient: inline-axis;-webkit-box-pack: start;padding: 15px 15px 20px 0;}
.autoList div{ margin-right:9%;}
.autoList .f13px{ line-height: 13px;}
.autoList .f15px{ line-height: 15px; margin-top: 10px;}
.autoList .states{position: inherit !important;right: 0;top: 0;}
.ico-plus{display: inline-block;width: 25px;height: 25px;background: url(/imgs/images/ico_plus.png) no-repeat;background-size: 25px 25px;vertical-align: -7px;}
.bot a{color: #1fcafb;}


/*自动投标规则*/
.auto-invest-rule{padding: 16px 16px 60px;}
.auto-invest-rule p{ font-size: 15px; color: #212121;text-align: justify; line-height: 25px; padding-bottom: 6px;}
.auto-invest-rule .ti25{text-indent:25px; }


@media screen and (max-width:320px) {
	.autoBox .infoBox p{ font-size: 1rem!important;}
}

@media screen and (device-aspect-ratio: 40/71) {
	.autoList div{ margin-right:5%;}
	.ico-arrow-d-ccc{bottom: 40px;}
	}

@media (device-width:375px) and (-webkit-min-device-pixel-ratio:2){
	.autoList div{ margin-right:9%;}
	}
@media (device-width:414px) and (-webkit-min-device-pixel-ratio:3.0){
	.autoList div{ margin-right:12%;}
	}













