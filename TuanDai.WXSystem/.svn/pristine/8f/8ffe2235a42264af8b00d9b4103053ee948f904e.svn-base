@charset "utf-8";
html,body,h1,h2,h3,p,span,a,div,ol,ul,li,dl,dt,dd,table,tbody,tfoot,thead,tr,th,td,input,textarea, form,article,aside,details,figcaption,figure,footer,header,hgroup,nav,section ,input{
    margin: 0; padding: 0;  -webkit-tap-highlight-color: rgba(0,0,0,0); box-sizing: border-box;
    -webkit-text-size-adjust: none; -webkit-user-select: none; -ms-user-select: none; user-select: none;}
body { font-size:16px;font-family:"微软雅黑", "宋体", Arial, Helvetica, sans-serif;background:#dddde0;-webkit-text-size-adjust: none; width: 100%;height:100%;-webkit-tap-highlight-color:rgba(0,0,0,0);}
html { height: 100%; width: 100%; font-size: 100px; outline: 0; -webkit-text-size-adjust:none;}
body { height: 100%; margin: 0; -webkit-user-select: none; position: relative;}
em, i, b, strong { font-style: normal; font-weight: normal;}
ul, li {list-style: none;}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section{display: block;}
img {  border:0; -ms-interpolation-mode:bicubic; max-width:100%; vertical-align:middle;}
input{-webkit-border-radius: 0; appearance:none;-moz-appearance:none; -webkit-appearance:none;}
a{text-decoration: none;outline: none;}
.pos-r{ position: relative !important;}
.pos-a{ position: absolute !important;}
.pos-f{ position: fixed !important;}
.imgBox{ text-align: center;}
.fl{float:left !important;}
.fr{float:right !important;}
.clearfix {zoom:1;}
.clearfix:after{content:'.'; display:block; visibility:hidden; clear:both; height:0;}
.t-a{text-align: center;}
.mt20{ margin-top: 0.2rem;}
table td{padding: 0.06rem 0;}
.text-l{text-align: left!important;}
.red{ color: red!important;}
.webkit-box{display: -webkit-box;}
.box-flex1{-webkit-box-flex: 1;box-flex: 1;}

/*头部返回*/
.headerMain{ height: 0.88rem; width: 100%; position: relative; z-index: 500; }
.headerMain .none{ height: 0.88rem; width: 100%; position: absolute; top: 0; left: 0; z-index: 10;}
.header{ width: 100%; height: 0.88rem;padding:0 0.2rem;background-color: #fff;opacity: 0.95; position:absolute; top: 0; left: 0; z-index: 50;}
.header .back{width: auto; height: 0.88rem; line-height: 0.88rem; color: #000;font-size: 0.3rem; position: absolute; top: 0; left: 0.15rem; z-index: 100; cursor:pointer;}
.header .back:before{content: " "; display: inline-block; width: 10px; height: 18px; vertical-align: middle;background: url(/imgs/images/back_Arrow.png) no-repeat; background-size: 10px 18px; vertical-align: middle; margin-right: 5px;margin-top: -3px;}
.header .title{ text-align: center; width: auto; height: 48px; line-height: 48px;color: #212121; margin: 0 auto;font-size: 0.3rem;}

.Contract{width:7.1rem;padding:0.3rem;margin:0.2rem auto 0;background:url(../images/Contract_bg.jpg?v=201706160001) top center repeat-y;background-size:7.1rem auto;line-height:22px;overflow:hidden;}
.gzWrap{padding:0.3rem 0.3rem 1.5rem 0.3rem;}
.Contract h3{font-size:0.32rem;padding:0;text-align:center;font-weight:bold; margin-bottom: 0.2rem;}
.Contract .Number{padding:0;text-align:right; margin-top: 0.2rem; font-size: 0.28rem;}
.Contract .Contract_t{height:auto;padding:0;text-align:left;line-height:30px;float:left;}
.Contract .Contract_t ul{list-style:none;padding:0.2rem 0;margin:0;float:left;}
.Contract .Contract_t ul li{padding:0;}
.Contract .Contract_t ul li div{ font-size: 0.28rem;}
.Contract .Contract_t ul li div span{ color: #666666;}
.Contract .Contract_c{padding:0;margin:0;text-align:left;line-height:190%;float:left;font-size: 0.265rem;}
.Contract .Contract_c p{padding:0;margin:0;line-height:190%;/*float:left;*/ text-align: justify; font-size: 0.25rem;}
.Contract .Contract_c .title{ font-size: 0.27rem;padding: 5px 0; line-height: 14px;}
.Contract .Contract_c span{padding:0 0.08rem;color:#666;text-decoration:underline;}
.Contract .Contract_c em{padding:0 0.08rem; font-style:normal;}
.Contract .Contract_b{width:6.5rem;padding:0;margin:0;text-align:left;line-height:190%;float:left;position:relative;z-index:999}
.Contract .Contract_b p{width:6.5rem;padding:0;margin:0;text-align:left;line-height:190%;float:left;}
.Contract .Contract_b span{padding:0 0.08rem;color:#666;text-decoration:underline;} 
.Contract .Contract_c .spDeadType { padding: 0 5px; color: #000000;  text-decoration:none; }
.Contract .Contract_b strong{padding:0;}
.Contract .Contract_b em{padding:0 5px; font-style:normal;}
