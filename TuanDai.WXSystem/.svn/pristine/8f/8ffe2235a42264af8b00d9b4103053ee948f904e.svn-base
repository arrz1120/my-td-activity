/*导航协议切换*/
.bb-c7c7ca{ position: relative;}
.bb-c7c7ca:after{content: "";position: absolute;left: 0;bottom: 0;right: 0;border-bottom: 1px solid #c7c7ca;transform: scaleY(.5);transform-origin: 0 100%;}
.nav{width: 100%; height: 0.9rem; line-height: 0.9rem;left: 0;top:0;z-index: 10000; background: #fff;}
.nav a{display: block;text-align: center;font-size: 0.34rem; color: #999999;}
.nav + .Contract{ margin-top: 1.2rem;}
.nav a span{width: 100%;font-size: 0.34rem; height: 0.34rem; line-height: 0.34rem; color: #999999; position: relative;display: inline-block; margin-top: 0.3rem;}
.nav a span:after{content: "";position: absolute;top: 0;bottom: 0;right:0;border-right: 1px solid #ccc;transform: scaleX(.5);transform-origin: 0 0;}
.nav .active span{ color: #fab600;}