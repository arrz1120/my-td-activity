            <tr>
               <td style="font-size:0.28rem;width:50%">甲方：</td>
               <td style="font-size:0.28rem;">乙方：</td>
           </tr>
           <tr>
               <td style="font-size:0.28rem;">日期：</td>
               <td style="font-size:0.28rem;">日期：</td>
           </tr>
        </table>
