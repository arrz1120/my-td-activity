<script type="text/javascript">
	$(".spread-control").click(function(){
		var $spread = $("#spread");
		if($spread.hasClass('spread')){
			$spread.removeClass('spread');
			$(this).find('img').css('transform','rotateZ(180deg)');
		}
		else{
			$spread.addClass('spread');
			$(this).find('img').css('transform','rotateZ(0)');
		}
	})
</script>
