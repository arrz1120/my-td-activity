	//收益切换
	$("#ruleTab1").click(function(){
		if($(this).hasClass('active') == false){
			$(this).addClass('active');
			$("#ruleTab2").removeClass('active');
			$("#ruleCon2").addClass('hide');
			$("#ruleCon1").removeClass('hide');
		}
	})
	$("#ruleTab2").click(function(){
		if($(this).hasClass('active') == false){
			$(this).addClass('active');
			$("#ruleTab1").removeClass('active');
			$("#ruleCon1").addClass('hide');
			$("#ruleCon2").removeClass('hide');
		}
	});
	
	//
	$(".dropdownTit").click(function(){
		var next = $(this).next();
		var triangle = $(this).find('i');
		if(next.hasClass('hide')){
			triangle.removeClass('rotate0');
			next.removeClass('hide');
		}else{
			triangle.addClass('rotate0');
			next.addClass('hide');
		}
	})
	
	$(".ico-doubt").click(function(){
		$("#wex-alert").removeClass('hide');
	});
	
	$("#iKnow").click(function(){
		$("#wex-alert").addClass('hide');
	})

	//投资弹框
    function moveToTop(ele){
    	$(ele).removeClass('moveToBottom').removeClass('hide').addClass('moveToTop');
    }
    function moveToBottom(ele){
    	$(ele).removeClass('moveToTop').addClass('moveToBottom');
    	setTimeout(function(){
    		$(ele).addClass('hide');
    	},500);
    }
    $("#join").click(function(){
		moveToTop("#invest_process");
    });
    $("#detail-close").click(function(){
    	moveToBottom("#invest_process");
    });



    var scrollT="";
    function rpmoveToTop(open,target){
        $(open).click(function(){
            scrollT = $(window).scrollTop();
            $(target).removeClass('hide').removeClass('moveToBottom').addClass('moveToTop');
            $('.rp-ways').stop().animate({ scrollTop: 0 }, 10);
            setTimeout(function(){
                $("#bigDiv").hide();
            },400);
        });
    }

    function rpmoveToBottom(close,target){
        $(close).click(function(){
            $("#bigDiv").show();
            $(window).scrollTop(scrollT);
            $(target).removeClass('moveToTop').addClass('moveToBottom');
            setTimeout(function(){
                $(target).addClass('hide');
            },400);
        })
    }

      $("#ues_rp").click(function () {
          $("#invest_process").addClass("hide");
      });

    rpmoveToTop("#ues_rp",'#rp_ways');
    rpmoveToBottom('#rpways_close','#rp_ways');

