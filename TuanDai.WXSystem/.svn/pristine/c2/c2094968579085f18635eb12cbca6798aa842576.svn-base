<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="pay_post.aspx.cs" Inherits="TuanDai.WXApiWeb.PaymentPlatform.Baofu.pay_post" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>充值接口-提交信息处理</title>
</head>
<body onload='document.pay.submit()'>
    <form name='pay' method="post" action="https://tgw.baofoo.com/paycerwap">
     <input name='MemberID' type='hidden' value= "<%=strMemberID%>"/>
	<input name='TerminalID' type='hidden' value= "<%=strTerminalID%>"/>
	<input name='InterfaceVersion' type='hidden' value= "<%=strInterfaceVersion%>"/>
	<input name='KeyType' type='hidden' value= "<%=strKeyType%>"/>
	<input name='PayID' type='hidden' value= "<%=strPayID%>"/>		
	<input name='TradeDate' type='hidden' value= "<%=strTradeDate%>" />
	<input name='TransID' type='hidden' value= "<%=strTransID%>" />
	<input name='OrderMoney' type='hidden' value= "<%=strOrderMoney%>"/>
	<input name='ProductName' type='hidden' value= "<%=strProductName%>"/>
	<input name='Amount' type='hidden' value= "<%=strAmount%>"/>
	<input name='Username' type='hidden' value= "<%=strUsername%>"/>
	<input name='AdditionalInfo' type='hidden' value= "<%=strAdditionalInfo%>"/>
	<input name='PageUrl' type='hidden' value= "<%=strPageUrl%>"/>
	<input name='ReturnUrl' type='hidden' value= "<%=strReturnUrl%>"/>	
	<input name='Signature' type='hidden' value="<%=strSignature%>"/>
	<input name='NoticeType' type='hidden' value= "<%=strNoticeType%>"/>
    </form>
    
</body>
</html>
