html,body{width: 100%;overflow-x: hidden;}
body{background: #fbfbf9;}

.newUserTips{background: #fff8e2;height: 40px;text-align: center;}
.newUserTips p{line-height: 40px;color: #c49e14;font-size: 1.2rem;}
.myPrivileges .banner{height: 150px;background:#ffcf1f;position: relative;padding: 10px 0;}
.myPrivileges .banner .triangle{display: block;width: 18px;height: 9px;background: url(/imgs/member/triangle.png) no-repeat;background-size: 100%;
position: absolute;bottom: 0;left: 50%;margin-left: -9px;}
.myPrivileges .itemOuter{border-bottom: 1px solid #e6e6e6;padding: 15px 20px 15px 15px;}
.myPrivileges .list .item{padding-left: 55px;}
.myPrivileges .list .item h3{font-size: 1.5rem;color: #000;font-weight: normal;}
.myPrivileges .list .item p{font-size: 1.2rem;color: #ababab;line-height: 2.2rem;}
