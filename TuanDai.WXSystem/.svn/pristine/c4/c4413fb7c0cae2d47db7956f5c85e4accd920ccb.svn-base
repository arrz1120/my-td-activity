.swiper-container{width: 100%;height: 100%;}
.swiper-slide{width: 25%;height: 100%;position: relative;}
.swiper-slide .userLevel{position: absolute;top: 0;width: 100%;text-align: center;}
.swiper-slide .userLevel img{max-width: 90px;}
.swiper-slide .pics{height: 60px;width: 100%;background: url(/imgs/member/shadow_s.png) center bottom no-repeat;background-size: 35px auto;
	position: absolute;top: 45px;}
.swiper-slide-active .pics{background: url(/imgs/member/shadow_b.png) center bottom no-repeat;background-size: 60px auto;top: 35px;height: 80px;}
.swiper-slide .pics .img{width: 50px;margin: 0 auto;}
.swiper-slide .pics .img_b{width: 70px;display: none;}
.swiper-slide-active .pics .img_s{display: none;}
.swiper-slide-active .pics .img_b{display: block;}
