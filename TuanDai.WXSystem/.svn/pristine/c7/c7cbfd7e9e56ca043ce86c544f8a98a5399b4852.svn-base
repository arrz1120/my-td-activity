@charset "utf-8";
html, body, h1,h3, p, span, a, div,input,form { margin: 0; padding: 0; font: 12px/2em 'Microsoft Yahei'; -webkit-tap-highlight-color: rgba(0,0,0,0); color: #000;background: #ededed;}
body { background: #ededed; -webkit-text-size-adjust: none; width: 100%;display: -webkit-box;}
input[type='text'], input[type='password']{-webkit-border-radius: 0; -webkit-appearance: none;height: 44px;line-height: normal;margin-top: 7px;border: 1px solid #cbcbcb;font-size: 1.35rem;width: 85%;padding-left: 15%;-webkit-border-radius: 4px;-moz-border-radius: 4px;border-radius: 4px;outline: 0;
    background: #fff;}
.bid-sum input::-webkit-input-placeholder{color: #ababab;}
input:hover{border:1px solid #ffc800;}
a{text-decoration: none;}
img {border:0; -ms-interpolation-mode:bicubic; max-width:100%; vertical-align:middle;}
.icon{background: url(../imgs/icon.png) no-repeat;background-size: 171px auto;position: absolute;}
.warp{width: 100%;height: 100%;}
.warp header{ background: #ffc800; height: 52px;}
.warp header img{display: block;  width:103px;margin: 0 auto; padding-top: 13px;}
.main{width: 93.75%;margin: 0 auto;}
.main .items{margin-top: 23px;position: relative;}
.items label{display: block;font-size: 1.45rem;}
.items b{left: 10px;top: 45px;}
.items .i1{width: 19px;height: 22px;background-position:0 -70px;}.items:hover .i1{background-position:-25px -70px;}
.items .i2{width: 18px;height: 22px;background-position:0 -95px;top: 47px;}.items:hover .i2{width: 19px;height: 24px;background-position:-25px -95px;}
.items .i3{width: 19px;height: 19px;background-position:0 -175px;top: 50px;}.items:hover .i6{background-position:-25px -175px;}
.items .t1{ position: absolute; font-size: 0.9rem;color: #e84419; display: none; }
.items .t1 img{ width: 12px; height: 12px; margin-top: -4px;display: inline-block;margin-right: 5px;}
.items .t2{ position: absolute; font-size: 0.9rem;color: #e84419; display: none;}
.items .t2 img{ width: 12px; height: 12px; margin-top: -4px;display: inline-block;margin-right: 5px;}
.items .verifydtxt{width: 41%;}
.items .code{width: 41%;height: 45px;-webkit-border-radius: 4px;-moz-border-radius: 4px;border-radius: 4px;
    border: solid 1px #cbcbcb;display: inline-block;text-align: center; margin-top: -4px;}
@media all and (min-device-width:320px) and (max-device-width: 360px){.items .verif-btn{width: 40.8%;}}
.logo-cn .logo-btn{width: 100%;height:44px;line-height:44px;background: #f5b11d; border: 0;color: #fff; font-size: 1.6rem; margin-top: 27px;
    -webkit-border-radius: 4px;-moz-border-radius: 4px;border-radius: 4px;}
.logo-cn .logo-btn:hover{border: 0;outline: none;}


/*绑定成功 star*/
.succeed{width: 93.75%;margin: 0 auto;}
.top-cn{padding-bottom: 30px;display: -moz-box;display: -webkit-box;display: box;padding-top: 60px;}
.bottom-cn{padding: 5px 0 120px 0;}
.info-main{-webkit-box-flex: 2;-ms-flex: 2;box-flex: 2;margin-left: 10px;}
.tick{-webkit-box-flex: 1;-ms-flex: 1;box-flex: 1;margin: 5px 0 0 15px;}
.tick p{border-radius: 50%; -webkit-border-radius:50%; -mon-border-radius:50%;width: 60px;height:60px;background: #35c406;border: solid 1px #28ab00;
    background-image: -webkit-linear-gradient(bottom, rgba(0,0,0,.3), rgba(255,255,255,.3));
    background-image: -moz-linear-gradient(bottom, rgba(0,0,0,.3), rgba(255,255,255,.3));
    background-image: -o-linear-gradient(bottom, rgba(0,0,0,.3), rgba(255,255,255,.3));
    background-image: linear-gradient(to top, rgba(0,0,0,.3), rgba(255,255,255,.3));}
.tick p img{width: 31px;height: 26px;padding:15px}
.suc-info{color: #212121;font-size: 1.75rem; margin-top: 16px;}
.but-cn a{width: 100%;height: 44px;line-height: 44px;font-size: 1.6rem;-webkit-border-radius: 4px;-moz-border-radius: 4px;border-radius: 4px; color: #fff;outline: none;
display: inline-block;text-align: center; }
.but-cn .suc-but1{background: #f5b11d;}.but-cn .suc-but1:hover{background: #808080;}
