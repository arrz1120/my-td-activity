@charset "utf-8";
html,body,h1,h2,h3,p,span,a,div,ol,ul,li,dl,dt,dd,table,tbody,tfoot,thead,tr,th,td,input,textarea, form,article,aside,details,figcaption,figure,footer,header,hgroup,nav,section { color: #ffffff;margin: 0; padding: 0; -webkit-tap-highlight-color: rgba(0,0,0,0);box-sizing: border-box;}
body { background: #ffffff; -webkit-text-size-adjust: none;-webkit-user-select:none;width: 100%;height:100%;-webkit-tap-highlight-color:rgba(0,0,0,0);-webkit-user-select:none;font-size: 100px;font-family: "微软雅黑";}
em, i, b, strong { font-style: normal; font-weight: normal;}
input, textarea { outline: 0;}
input,textarea,select{-webkit-border-radius: 0; appearance:none;-moz-appearance:none; -webkit-appearance:none;}
ul, li {list-style: none;}
fieldset{border: none;}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section{display: block;}
img {  border:0; -ms-interpolation-mode:bicubic; max-width:100%; vertical-align:middle;}
a{text-decoration: none;}
input::-webkit-input-placeholder,textarea::-webkit-input-placeholder {color: #ababab;}
input:-webkit-autofill { -webkit-box-shadow: 0 0 0 1000px white inset;}

.hide{display: none !important;}
.pos-r{ position: relative;}
.pos-a{ position: absolute;}
.pos-f{ position: fixed;}
.clearfix {zoom:1;}
.clearfix:after{content:'.'; display:block; visibility:hidden; clear:both; height:0;}
.fl{float: left;}
.fr{float: right;}
.block{display: block!important;}
.text-center{text-align: center;}
.bg-size{background-size: 100% 100%;background-repeat: no-repeat;}
.rotateZ180{-webkit-transform: rotateZ(-180deg);}

.content-box{box-sizing: content-box;}
.webkit-box{display: -webkit-box;}
.box-center{display: -webkit-box;-webkit-box-align: center;-webkit-box-pack: center;}
.box-vertical{-webkit-box-orient: vertical;}
.justify-content{display: flex; justify-content: center;}
.display-flex{display: flex; }
.box-vertical{-webkit-box-orient: vertical;}
.box-flex1{-webkit-box-flex: 1;box-flex: 1;}

.text-indent{text-indent: 0.4rem;}
.opacity1{opacity: 1;}
.text-overflow{white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}

body{ overflow: hidden;}
.scroll { margin: 0 auto; overflow-x: hidden; overflow-y: auto; }
.scroll, body, html { height: 100%; position: relative; width: 100%; }
.scroll-active { -webkit-overflow-scrolling: touch!important; }

.wrapper{ margin-left: 0.3rem;}
.border-b{ border-bottom:0.02rem solid #e6e6e6;padding: 0.3rem 0;}
.border-b .title{ font-size: 0.3rem; color: #999999;}
.border-b .time{ font-size: 0.24rem; color: #ababab; margin-top: 0.15rem;}
.notice-con{padding-right: 0.3rem; padding-top: 0.3rem;}
.notice-con p{text-align: justify; font-size: 0.3rem; color: #333333;  line-height: 180%;}
.notice-con p img{display: block;padding: 0.2rem 0;}
.notice-con a{display: block;font-size: 0.3rem; color: #ffb700;line-height: 180%;}






