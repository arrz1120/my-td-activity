input{border: none;outline: none;}
.logSprite{background:url(/imgs/images/icon/log.sprite.png)  no-repeat;background-size:126px 17px}
.phone{height:17px;width:12px;background-position:0 0;}
.close{height:12px;width:12px;background-position:-15px 0;}
.unlock{height:16px;width:15px;background-position:-29px 0;}
.info{height:13px;width:13px;background-position:-46px 0;margin-top: 3px;}
.eye-close{height:6px;width:14px;background-position:-61px 0;}
.eye-open{height:10px;width:14px;background-position:-77px 0;}
.people{height:13px;width:14px;background-position:-95px 0;margin-top: 2px;}
.lock{height:32px;width:26px;background-position: -113px 0;}

.log-alert{width: 100%;height: 100%;top: 0;left: 0;position: fixed;z-index: 999;}
.log-close{position: absolute;left: 15px;top: 15px;width: 30px;height: 30px;}
.contentWrap{padding: 0 45px;}
@media only screen and (max-width: 360px) {
	.contentWrap{padding: 0 35px;}
}
@media only screen and (max-width: 320px) {
	.contentWrap{padding: 0 15px;}
}
.input-box{width: 100%;position: relative;height: 40px;margin-top: 18px;}
.input-box i{position: absolute;top: 11px;left: 10px;}
.input-box .btnsee{position: absolute;right: 10px;top: 0;width: 40px;height: 40px;}
.input-box input{height: 39px;width: 100%;font-size: 15px;color: #212121;padding-left: 40px;}
.input-box p{font-size: 15px;color: #ababab;position: absolute;left: 40px;top: 0;display: block;height: 39px;line-height: 39px;}
.input-box p span{font-size: 11px;color: #ababab;}
.timeout,.timeWrap{width: 103px;text-align: center;position: absolute;right: 0;top: 10px;height: 20px;}
.time_bg{position: absolute;right: 39px;top: -3px;width: 26px;height: 26px;border: 2px solid #e6e6e6;border-radius: 50%;}
.time{position: absolute;right: 39px;top: -3px;width: 26px;height: 26px;}
.time .wrap{width: 13px;height: 26px;position: absolute;top:0;overflow: hidden;}
.time .circle_right{right:0;}
.time .circle_left{left:0;}
.time .round{width: 26px;height: 26px;border:2px solid transparent;border-radius: 50%;position: absolute;top:0;-webkit-transform : rotate(-135deg);transform : rotate(-135deg);}
.time .rightcircle{border-top:2px solid #ffcf1f;border-right:2px solid #ffcf1f;right:0;}
.time .leftcircle{border-bottom:2px solid #ffcf1f;border-left:2px solid #ffcf1f;left:0;}
#show{text-align: center;line-height: 26px;font-size: 10px;color: #ababab;height: 26px;}
.circle_process{position: relative;width: 26px;height : 26px;}
.circle_process .wrapper{width: 13px;height: 26px;position: absolute;top:0;/*overflow: hidden;*/}
.circle_process .circle_right{right:0;}
.circle_process .circle_left{left:0;}
.circle_process .round{width: 26px;height: 26px;border:2px solid transparent;border-radius: 50%;position: absolute;top:0;transform : rotate(0);}
.circle_process .rightcircle{border-top:2px solid red;border-right:2px solid red;right:0;/*-webkit-animation: circle_right 5s linear infinite;*/}
.circle_process .leftcircle{border-bottom:2px solid #ffcf1f;border-left:2px solid #ffcf1f;left:0;/*-webkit-animation: circle_left 5s linear infinite;*/}
@-webkit-keyframes circle_right{
    0%{-webkit-transform: rotate(0);}
    50%,100%{-webkit-transform: rotate(180deg);}
}
@-webkit-keyframes circle_left{
    0%,50%{-webkit-transform: rotate(180);}
    100%{-webkit-transform: rotate(0);}
}

.invite_t .rf{width: 103px;text-align: center;}
.triangle-r{
	width: 0;height: 0;
	border-top: 5px solid transparent;border-bottom: 5px solid transparent;border-left: 10px solid #ffcf1f;
	display: inline-block;margin-right: 5px;
}
.triangle-d{
	width: 0;height: 0;
	border-right: 5px solid transparent;border-left: 5px solid transparent;border-top: 10px solid #ffcf1f;
	display: inline-block;margin-right: 5px;
}
.tdLogo{width: 75px;height: 75px;margin: 60px auto 0;background: url(/imgs/images/bg/tuandai_logo.png) no-repeat;background-size: 75px 75px;}
.bottom_txt{position: absolute;bottom: 10px;}
.bottom_txt span{margin: 0 3px;}

.opacity0{opacity: 0;}
.opacity05{opacity: 0.5;} 
.noPhoneCode{color: #999;font-size: 13px;text-align: right;padding-right: 20px;margin-top: 5px;}
/*动画*/
.moveToTop{-webkit-animation: moveToTop ease-out 0.2s forwards;}
.moveToBottom{-webkit-animation: moveToBottom ease-out 0.2s forwards;}
@-webkit-keyframes moveToTop{
	from{-webkit-transform: translate3d(0,100%,0);opacity: 0;}
	to{-webkit-transform: translate3d(0,0,0);opacity: 1;}
}
@-webkit-keyframes moveToBottom{
	from{-webkit-transform: translate3d(0,0,0);opacity: 1;}
	to{-webkit-transform: translate3d(0,100%,0);opacity: 0;}
}