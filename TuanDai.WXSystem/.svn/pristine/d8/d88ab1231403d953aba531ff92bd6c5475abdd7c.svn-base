/*动画*/
.animated { -webkit-animation-duration: 1s; animation-duration: 1s; -webkit-animation-fill-mode: both; animation-fill-mode: both }
.animated.infinite { -webkit-animation-iteration-count: infinite; animation-iteration-count: infinite }
.p1_1,.p1_4,.p1_5,.p2_1,
.p2_3,.p2_2,
.p3_1,.p3_3,
.p4_1,.p4_2,
.p5_1,.p5_3,.p5_4,.p5_5,.p5_6{opacity:0;}

.animated.bounceIn.p1_2{-webkit-animation-iteration-count: 1;-webkit-animation-delay: 1.2s;-webkit-animation-duration: 0.6s;}
.animated.fadeIn1.p1_3{-webkit-animation-iteration-count: 1;-webkit-animation-delay: 1.2s;-webkit-animation-duration: 1.5s;}
.animated.bounce.p1_3cn{-webkit-animation-iteration-count: 2;-webkit-animation-delay: 1.4s;-webkit-animation-duration: 1.4s;}
.p1_4{opacity: 1;-webkit-animation: opaTop 1.4s 1s linear both; animation: opaTop 1.4s 1s linear both;}
.p1_5{opacity: 1;-webkit-animation: opaLeft 1.2s 1.4s ease-in-out both;animation: opaLeft 1.2s 1.4s ease-in-out both;}

.active .p2_1,.active .p3_1,.active .p4_1,.active .p5_1
{opacity: 1;-webkit-animation: opaBottom 1.2s .4s ease-in-out both; animation: opaBottom 1.2s .4s ease-in-out both;}
.active .p2_2,.active .p3_2,.active .p5_2{-webkit-animation-iteration-count: 1;-webkit-animation-delay: 1.5s;-webkit-animation-duration: 3s; }
.active .p2_2 img,.active .p3_2 img,.active .p5_2 img{-webkit-animation-iteration-count: 1;-webkit-animation-delay: 1s;-webkit-animation-duration: 1.8s; }

.active .p2_4,.active .p3_4,.active .p4_4,.active .p5_4,.active .p6_4,.active .p7_4
{opacity: 1;-webkit-animation: opaTop 1.2s .8s linear both; animation: opaTop 1.2s .8s linear both;}
.active .p2_5,.active .p3_5,.active .p4_5,.active .p5_5,.active .p6_5,.active .p7_5
{opacity: 1;-webkit-animation: opaRight 1.4s 1.2s ease-in-out both;animation: opaRight 1.4s 1.2s ease-in-out both;}

.active .p2_3,.active .p3_3,.active .p4_3,.active .p5_3
{opacity: 1;-webkit-animation: opaRight 1.2s 1.6s ease-in-out both;animation: opaRight 1.2s 1.6s ease-in-out both;}
.active .p2_3 img,.active .p3_3 img,.active .p4_3 img,.active .p5_3 img{
    -webkit-animation-iteration-count: 10;-webkit-animation-delay: 1.5s;-webkit-animation-duration: 3s;display: inline-block;}

.active .p2_6,.active .p3_6,.active .p4_6,.active .p5_6
{-webkit-animation-iteration-count: 1;-webkit-animation-delay: 3s;-webkit-animation-duration: .6s;}
.active .p2_7,.active .p3_7,.active .p4_7,.active .p5_7
{opacity: 1;-webkit-animation: opaRight 1.2s 2.7s ease-in-out both;animation: opaRight 1.2s 2.7s ease-in-out both;}
.active .p2_8,.active .p3_8,.active .p4_8,.active
{-webkit-animation-iteration-count: 1;-webkit-animation-delay: 4.2s;-webkit-animation-duration: .6s;}

.active .p4_9{opacity: 1;-webkit-animation: opaBottom 1.4s 3s ease-in-out both; animation: opaBottom 1.4s 3s ease-in-out both;}


@-webkit-keyframes jump{
    0%{-webkit-transform:scale(1,1);}
    5%{-webkit-transform:scale(1.03,.8);}
    10%{-webkit-transform:scale(.9,1.06);}
    15%{-webkit-transform:scale(1.06,.8);}
    20%{-webkit-transform:scale(.85,1.1);}
    25%{-webkit-transform:scale(1.04,.9);}
    30%{-webkit-transform:scale(.95,1.02);}
    35%{-webkit-transform:scale(1,1);}
    100%{-webkit-transform:scale(1,1);}}
.active .share{-webkit-animation:jump 5.8s linear 1.2s infinite;}

