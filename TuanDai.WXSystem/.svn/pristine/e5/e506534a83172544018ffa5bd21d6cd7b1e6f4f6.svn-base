@charset "utf-8";
html,body,h1,h2,h3,p,span,a,div,ol,ul,li,dl,dt,dd,table,tbody,tfoot,thead,tr,th,td,input,textarea, form,article,aside,details,figcaption,figure,footer,header,hgroup,nav,section { margin: 0; padding: 0; font: 12px/2rem 'Microsoft Yahei', Arial, Verdana, Helvetica, sans-serif; -webkit-tap-highlight-color: rgba(0,0,0,0); color: #808080;box-sizing: border-box;}
body { background: #fbfbf9; -webkit-text-size-adjust: none; width: 100%;height:100%;-webkit-tap-highlight-color:rgba(0,0,0,0);}
em, i, b, strong { font-style: normal; font-weight: normal;}
input, textarea { outline: 0;}
input,textarea,select{-webkit-border-radius: 0; appearance:none;-moz-appearance:none; -webkit-appearance:none;}
ul, li {list-style: none;}
fieldset{border: none;}
article,aside,details,figcaption,figure,footer,header,hgroup,nav,section{display: block;}
img {  border:0; -ms-interpolation-mode:bicubic; max-width:100%; vertical-align:middle;}
.br-ccc,.bt-c2c2c2,.bb-c2c2c2{position: relative;}
.br-ccc:after{content: "";position: absolute;top: 0;bottom: 0;right: 0;border-right: 1px solid #cccccc;transform: scaleX(.5);transform-origin: 0 0;}
.bt-c2c2c2:before{content: "";position: absolute;left: 0;top: 0;right: 0;border-bottom: 1px solid #c2c2c2;transform: scaleY(.5);transform-origin: 0 0;}
.bb-c2c2c2:after{content: "";position: absolute;left: 0;bottom: 0;right: 0;border-bottom: 1px solid #c2c2c2;transform: scaleY(.5);transform-origin: 0 100%;}
.pt15{ padding-top: 15px;}
.pt20{ padding-top: 20px;}
.pb20{ padding-bottom: 20px;}

/*盒模型*/
.content-box{box-sizing: content-box;}
.webkit-box{display: -webkit-box;}
.box-flex1{-webkit-box-flex: 1;box-flex: 1;}
.pos-r{ position: relative !important;}
.pos-a{ position: absolute !important;}
.pos-f{ position: fixed !important;}

/*浮动、清除浮动*/
.lf{float:left;}
.rf{float:right;}
.clearfix {zoom:1;}
.clearfix:after{content:'.'; display:block; visibility:hidden; clear:both; height:0;}

body{ background: #f2f2f2!important;}
.block{display: block!important;}
.swiper-slide{height:350px}
.swiper-slide-active { height:auto}

.swiper-container{margin: 10px auto 0!important;}
.main{padding: 0 15px 20px 15px;background: #fff; }
.main p{ font-size: 13px;line-height: 26px;}

.tabs{ background: #fff; height: 45px; line-height: 45px;}
.tabs a{display: block;height: 45px; line-height: 45px;text-decoration: none;text-align:right }
.tabs a span{color: #666666; font-size: 15px;height: 20px; padding-right: 30px;}
.tabs a.active span{ color: #ffc61a;}
.con .clearfix{ padding-bottom:5px;}
.con .lf{width: 25%;}
.con .rf{width: 75%;}

.main h3{ padding: 10px 0 5px 0; font-size: 15px; color: #212121;}
.main .item{ padding-left: 20px;}
.m1{ margin-bottom: 20px;}


