/*页面加载进度条*/
.wb-fix { position: fixed; left: 0; right: 0; bottom: 0; top: 0; padding: 0 3%; background-color: rgba(0, 0, 0, 0.5); z-index: 10000;-webkit-box-pack: center; box-pack: center; -webkit-box-align: center; box-align: center; display: none; }
.wb-fix.show { display: -webkit-box; display: box; }
.wb-fix.cm-loading { background: none; z-index: 100; }
.wb-fix.cm-loading .load { background: rgba(0, 0, 0, 0.5); width: 1.4rem; height: 1.4rem; padding: 0.3rem; border-radius: 0.2rem; }

.loading-box{height: 1.02rem;position: absolute;top: 50%;margin-top: -0.52rem;text-align: center;display:-webkit-box;-webkit-box-pack:center; -webkit-box-align:center;width: 100%;z-index:10000;}
.loading-tips{padding:0.26rem 0.3rem;background: rgba(255,255,255,0.95);border-radius: 0.08rem;box-shadow: 0.02rem 0.04rem 0.2rem 0px rgba( 0, 0, 0 ,0.15);}
.loading-tips img{width: 0.44rem;height: 0.44rem;display: inline-block;vertical-align: -0.12rem;-webkit-animation: loading 0.5s linear infinite;}
.loading-tips span{display: inline-block;font-size: 0.3rem;color: #808080;margin-left: 0.28rem;}
@-webkit-keyframes loading {
    from {-webkit-transform:rotate(0deg) translateZ(0); }
    to {-webkit-transform:rotate(360deg) translateZ(0); }
}

.float-jump{position: fixed;right: 0.3rem;bottom: 0.4rem;z-index: 1000;}
.float-jump a{display: block;width: 0.98rem;height: 0.98rem;}

.alert{position: fixed;top: 0;left: 0;width: 100%;height: 100%;z-index: 99;display: -webkit-box;-webkit-box-align: center;-webkit-box-pack: center;background: rgba(0,0,0,0.3);}
.alert_con{text-align: center;background: #fff;width: 84%;border-radius: 0.2rem;z-index: 2;}
.alert_con p{font-size: 0.34rem;color: #212121;padding: 0.4rem 0.5rem;}
.iKnow{font-size: 0.34rem;color: #ffb339;text-align: center;height: 0.8rem;line-height: 0.8rem;border-top: 1px solid #e6e6e6;}
.aniFadeIn{-webkit-animation: aniFadeIn 0.4s cubic-bezier(0.7,0,0.3,1) both;}
.aniFadeOut{-webkit-animation: fadeOut 0.4s cubic-bezier(0.7,0,0.3,1) both;}
.aniShow{-webkit-animation: aniShow 0.4s cubic-bezier(0.7,0,0.3,1) both;}
.aniHide{-webkit-animation: aniHide 0.4s cubic-bezier(0.7,0,0.3,1) both;}

@-webkit-keyframes aniFadeIn{
    0%{opacity: 0;}
    100%{opacity: 1;}
}
@-webkit-keyframes fadeOut{
    0%{opacity: 1;}
    100%{opacity: 0;}
}
@-webkit-keyframes aniShow{
    0%{-webkit-transform: translate3d(0,-400px,0);opacity: 0;}
    100%{-webkit-transform: translate3d(0,0,0);opacity: 1;}
}
@-webkit-keyframes aniHide{
    0%{-webkit-transform: translate3d(0,0,0);opacity: 1;}
    100%{-webkit-transform: translate3d(0,100px,0);opacity: 0;}
}