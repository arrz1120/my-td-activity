/*申请资产借款*/
.abs-inpBox{display: block;position: relative;padding-left: 120px;height: 44px;line-height: 44px;background: #fff;}
.abs-inpBox span{color: #212121;font-size: 17px;position: absolute;left: 0;top: 10px;}
.abs-inpBox input{height: 44px;font-size: 17px;border: none;outline: none;}
.abs-inpBox i{font-size: 17px;color: #212121;position: absolute;right: 15px;top: 0;}
.abs-inpBox img{width: 8px;height: 14px;position: absolute;right: 15px;top: 15px;}
.bottom-btn{position: fixed;bottom: 0;left: 0;height: 49px;line-height: 49px;width: 100%;}
#borrow-alert{bottom: 0;}
.error-tips{width: 86%;padding: 20px 0;text-align: center;color: #fff;font-size: 17px;position: fixed;left: 7%;background: rgba(0,0,0,0.7);border-radius: 8px;top: 50%;margin-top: -40px;}
.repayType{height: 60px;}
.repayType span{top: 18px;}
.repayType .repayType-select{width: 180px;height: 40px;border: 1px solid #ffd728;position: absolute;right: 15px;top: 10px;border-radius: 4px;}
.repayType .repayType-select span{display: block;width: 89px;height: 38px;line-height: 38px;text-align: center;color: #d1d1d1;position: absolute;top: 0;font-size: 15px;}
.repayType .repayType-select span:first-child{left: 0;}
.repayType .repayType-select span:last-child{left: 89px;}
.repayType .repayType-select span.active{background: #ffcf0a;color: #fff;}
/*动画*/
.moveToTop{-webkit-animation: moveToTop ease-out 0.4s forwards;}
.moveToBottom{-webkit-animation: moveToBottom ease-in 0.4s forwards;}
.error{height: auto !important;width: 100%; margin-bottom: 10px;margin-top:2px;}
.error .hint-text{ height: 28px;line-height:28px;background: #fcdcbd;-webkit-border-radius: 4px;border-radius: 4px;padding: 0 5px;}
.error .hint-text p{height: 28px; line-height: 28px;}
.error .hint-text img{width: 15px;}

@-webkit-keyframes moveToTop{
	from{-webkit-transform: translate3d(0,100%,0);opacity: 0;}
	to{-webkit-transform: translate3d(0,0,0);opacity: 1;}
}
@-webkit-keyframes moveToBottom{
	from{-webkit-transform: translate3d(0,0,0);opacity: 1;}
	to{-webkit-transform: translate3d(0,100%,0);opacity: 0;}
}
/*马上投资弹框*/
.ico-round-close{width: 24px;height: 24px;background: url(/imgs/images/ico-round-close.png) no-repeat;background-size: 100% 100%;left: 10px;top: 20px;}
.invest-alert-inp{height: 44px;line-height: 44px;border: 1px solid #ffcf1f;border-radius: 2px;}
