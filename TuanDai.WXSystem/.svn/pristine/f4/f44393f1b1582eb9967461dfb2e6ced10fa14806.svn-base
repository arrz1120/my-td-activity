/*确认投资*/
.wave_bg{height: 20px;padding-top: 5px;background: url(/imgs/images/bg_wave.png) repeat-x;background-size: auto 5px;}
.ico-elseMoney{display: block;width: 22px;height: 22px;margin-right: 10px;}
.ico-close-808080{width: 11px;height: 11px;background: url(/imgs/images/ico-close-808080.png) no-repeat;background-size: 11px 11px;right: 15px;top: 10px;}
.ico-payway{box-sizing:border-box;width: 18px;height: 18px;border: 1px solid #d4d4d4;border-radius: 50%;margin-right: 15px;vertical-align: -3px;}
.ico-payway-active{width: 18px;height: 18px;background: url(/imgs/images/yuan_active.png) no-repeat;background-size: 100% 100%;border: 0;}
.t_password{width: 100%;height: 41px;line-height: 41px;border: 1px solid #e6e6e6;border-radius: 3px;padding-left: 15px;font-size: 17px;color: #212121;}
.ico-eye{}
.ico-eye-ban{width: 20px;height: 15px;right: 15px;top: 15px;background: url(/imgs/images/eye_hui_ban.png) no-repeat;background-size: 100% 100%;}
.ico-eye-able{width: 20px;height: 12px;right: 15px;top: 15px;background: url(/imgs/images/eye_hui.png) no-repeat;background-size: 100% 100%;}

/*投资失败*/
.header .j-finish{width: auto; height: 48px; line-height: 48px; color: #000;font-size: 1.3rem; position: absolute; top: 0; right: 15px; z-index: 100; cursor:pointer;}
.invest-r-con{padding: 15px;background: #fff;}
.invest-r-con .con-top{ padding: 23px 0 22px;}
.invest-r-con .con-top img{display:block;width: 33.6%;margin: 0 auto 15px;}
.invest-r-con .con-top-main div:first-child{text-align: right; padding-right: 45px;}
.invest-r-con .con-top-main div:last-child{text-align: left; padding-left: 45px;}
.invest-r-con .con-bottom{ position: relative;padding: 15px;}
.invest-r-con .con-bottom:before{content: "";position: absolute;left: 0;top: 0;right: 0;border-bottom: 1px dashed #d1d1d1;transform: scaleY(.5);transform-origin: 0 0;}
.invest-r-con .con-bottom div{width: 100%;overflow: hidden;clear: both;}
.invest-r-con .con-bottom div p span{ margin-left: 20px;}
.invest-r-con .con-bottom div p:first-child{width: 50%;}
.invest-r-con .con-bottom div p:nth-child(2){ margin-left: 20px;}
.invest-r-con .j-con a:nth-child(1) img{width: 6%;vertical-align: -2px;}
.invest-r-con .j-con a:nth-child(2){ color: #ff6040;display: block;}
.invest-r-con .j-con p span{display: inline-block;}

/*投资成功*/
.bt-d1-dash,.bb-d1-dash{position: relative;}
.bt-d1-dash:before{content: "";position: absolute;left: 0;top: 0;right: 0;border-top: 1px dashed #d1d1d1;transform: scaleY(.5);transform-origin: 0 0;}
.bb-d1-dash:after{content: "";position: absolute;left: 0;bottom: 0;right: 0;border-top: 1px dashed #d1d1d1;transform: scaleY(.5);transform-origin: 0 100%;}
.suc_box{height: 80px;background: #fff;position: relative;z-index: 10;}
.suc_box p{font-size: 17px;}
.suc_box p i{display: inline-block;width: 30px;height: 30px;background: url(/imgs/images/icon/ico_suc02.png) no-repeat;background-size: 100% 100%;vertical-align: middle;margin-right: 10px;}
.whiteArea{height: 27px;background: #fff;}
.suc_frame{width: 92%;margin: -27px auto 0;position: relative;z-index: 5;}
.frame_line{height: 7px;width: 100%;background: url(/imgs/images/bg/bg_inv_line.png?v=20160603) no-repeat;background-size:100% 100%;border-radius: 20px;position: relative;z-index: 6;}
.frame_bg{width: 98.55%;margin: 0 auto 0;padding: 34px 10px 36px 10px;background: url(/imgs/images/bg/bg_inv_suc.png?v=20160603) center top no-repeat;background-size: 100% 100%;}
.frame_bg ul{margin: 0 10px;}
.frame_bg ul li{height: 40px;padding: 0 15px;}
.frame_bg ul li div{height: 40px;line-height: 40px;font-size: 14px;color: #808080;}
.frame_bg ul li div a{color: #fab600;font-size: 14px;}
.frame_bg ul li div i{right: 0;}
.moveFromBottom{opacity: 0;-webkit-animation-name: moveToTop;-webkit-animation-timing-function: ease-out;-webkit-animation-duration: 0.5s;-webkit-animation-fill-mode: forwards;}
.moveDelay1{-webkit-animation-delay: 0.6s;}
.moveDelay2{-webkit-animation-delay: 0.8s;}

.frame_bg{-webkit-animation: bgMove .6s ease-out forwards;animation: bgMove 0.6s ease-out forwards;}
@-webkit-keyframes bgMove{
	0%{-webkit-transform: translate3d(0,-200%,0);}
	100%{-webkit-transform: translate3d(0,0,0);}
}

@keyframes bgMove{
	0%{-webkit-transform: translate3d(0,-200%,0);}
	100%{-webkit-transform: translate3d(0,0,0);}
}

/*投资支付*/
.pt33{padding-top: 33px;}
.f36px{line-height: 36px;}
.i_c{padding: 25px 15px 15px 15px;}
.i_b{height: 43px;border: 1px solid #ffcf1f;border-radius: 5px;position: relative;padding-right: 40px;}
.i_b input{border: 0;outline: none;height: 100%;width: 100%;padding-left: 15px;color: #212121;font-size: 15px;border-bottom-left-radius: 5px;border-top-left-radius: 5px;}
.eye-box{position: absolute;right: 10px;top: 10px;width: 30px;height: 22px;}
