.invest-alert-inp span{display: block;box-sizing:border-box;width: 60px;height: 42px;line-height: 42px;text-align: center;top: 0;font-size: 2.5rem;border-left: 1px solid #ffcf1f;width: 17%;float: left;}
.invest-alert-inp span.add{color: #ffcf1f;right: 0;}
.invest-alert-inp span.minus{color: #ababab;right: 62px;}
@media only screen and  (max-width : 320px) {
	.invest-alert-inp input{font-size: 1.1rem;}
}
.invest_type ul li{height: 30px;line-height: 30px;border: 1px solid #d1d1d1;border-radius: 2px;font-size: 15px;color: #212121;text-align: center;width: 108px;float: left;margin-right: 10px;}
.invest_type ul li.active{background: #fffdf7 url(/imgs/images/invest_type_active.png) right top no-repeat;background-size: 13px 13px;border-color: #fab600;color: #fab600;}


.apply-suc-tips{color: #fff;font-size: 17px;width: 70%;height: 70px;line-height: 70px;position: absolute;left: 15%;top: 50%;margin-top: -35px;background: rgba(0,0,0,0.5);border-radius: 10px;}
.apply-suc-tips img{width: 45px;height: 45px;margin-left: 20px;margin-right: 10px;}

