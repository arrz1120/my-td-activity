.eye-box i.i_close{background: url(/imgs/images/eye_hui.png) no-repeat;background-size: 100% 100%;}
.eye-box i.i_open{background: url(/imgs/images/eye_huang.png) no-repeat;background-size: 100% 100%;}
.noMoney_tips{width: auto;height: 47px;background: url(/imgs/images/bg/inv_noMoney.png) no-repeat;background-size: 100% 100%;margin: 5px 30px 0 30px;text-align: center;font-size: 15px;color: #ff6040;padding-top: 13px;}
@media only screen and (max-width: 320px) {
	.noMoney_tips{margin: 5px 20px 0 20px;}
}

.code-popup{width: 84%;padding: 30px 0 21px 0;border-radius: 10px;}
.code-popup .close{display: block; width: 40px; height: 40px;top:0;right: 0;}
.code-popup .close i{background: url("/imgs/images/icon/ico-close.png") no-repeat;background-size: 12px; width: 12px; height: 12px;display: inline-block;margin: 14px; vertical-align: middle;}
.code-popup .code{ width: 60.3%;}
.code-popup .pic{width: 53.4%;}
.codeBox .code-popup{width: 66%;padding: 30px 0 25px 0;border-radius: 10px;}
.codeBox .code-popup .code{ width: 70%;}

.infoBox{ display: none; padding: 20px 15px; background-color: #fbfbf9; position: relative; z-index: 50;}
.infoBox a{ display: block; width: 100%; height: 100%; background: url('/imgs/images/ico_arrow_r.png') center right no-repeat; background-size: 10px 17px;}
.arrowDown{ display: block; width: 16px; height: 16px; background-color: #fff;transform:rotate(45deg);-webkit-transform:rotate(45deg); position: absolute; top: -8px; left: 50%; margin-left: -8px;}

.ico-plus{ display: inline-block; width: 25px; height: 25px; background: url('/imgs/images/ico_plus.png'); background-size: 25px; vertical-align:middle; margin-top: -2px;}
.auto-alert{width: 86%;border-radius: 10px;}

