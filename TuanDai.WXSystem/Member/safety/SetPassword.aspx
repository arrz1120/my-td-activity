<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="SetPassword.aspx.cs" Inherits="TuanDai.WXApiWeb.Member.safety.SetPassword" %>

<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
	<meta name="format-detection" content="telephone=no" />
	<meta name="apple-mobile-web-app-status-bar-style" content="black" />
	<title>设置登录密码</title>
	<link rel="stylesheet" type="text/css" href="/css/global.css?v=<%=TuanDai.WXApiWeb.GlobalUtils.Version %>" />
	<link rel="stylesheet" type="text/css" href="/css/range.css?v=20160305" />
	<link rel="stylesheet" type="text/css" href="/css/pay.css?v=20160305" />
</head>
<body class="bg-f6f7f8">
    <%= this.GetNavStr()%>
	<header class="headerMain">
	    <div class="header bb-c2c2c2">
	        <a class="back" href="BindMobileCode.aspx?tel=<%= tel %>">返回</a>
	        <h1 class="title">设置登录密码</h1>
	    </div>
        <%= this.GetNavIcon()%>
	    <div class="none"></div>
	</header>
	
	<div class="text-center">
		<div class="big-ico-box mt20">
			<img src="/imgs/images/big-ico02.png"/>
		</div>
		<p class="mt15 fb f17px c-212121">设置登录密码</p>
		<p class="mt15 f13px c-999999">请设置登录密码，保障账户安全</p>
	</div>
	
	<div class="clearfix inpBox4 bg-fff pt10 pb10 pl15 mt30">
		<div class="lf f17px c-212121">登录密码</div>
		<input type="text" class="lf f17px" placeholder="请设置登录密码" />
	</div>
	
	<div class="c-999999 f13px pl15 mt5">6~16个字符，需包含字母和数字</div>
	
	<div class="pl15 pr15 mt30">
		<a class="btn btnYellow">下一步</a>
	</div>
<script type="text/javascript" src="/scripts/jquery.min.js"></script>
<script type="text/javascript" src="/scripts/base.js?v=1"></script>
