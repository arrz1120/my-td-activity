	<script type="text/javascript">
	    var mySwiper = new Swiper('.swiper-container', {}); 
	    $('#v1,#v-btn').click(function () {
	        $("#v1,#v-btn").hide();
	        $('#v2').fadeIn(1000);
	        $(".video-box").show();
	        $(".video-box").width($(document.body).width() - 20);
	        $(".x-video-player").show();
	    });
	</script>

</html>