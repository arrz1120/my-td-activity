        $(".aboutTit").on("click", function () {
            if ($(this).parent(".aboutBox").hasClass("active")) {
                $(".aboutBox").removeClass("active");
            } else {
                $(".aboutBox").removeClass("active");
                $(this).parent(".aboutBox").addClass("active");
            }
        });
        ///////////////
        $(".certificate .smImgBox").click(function () {
            $(this).parent(".certificate li").find(".bigImgBox").show();
        });
        $(".bigImgBox").click(function () {
            $(this).hide();
        });

        $('#v1').click(function () {
            $("#v1").hide();
            $('#v2').fadeIn(1000);
            $(".video-box").width($(document.body).width() - 20);
        });

    });

</script>

