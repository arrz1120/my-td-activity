<script type="text/javascript">
  	$(function () {
		var swiperNav = new Swiper('#swiperNav', {
				slidesPerView: 'auto',
		        initialSlide: 6,
		        onSlideChangeStart:function(swiperNav){
		        	if(swiperNav.activeIndex>0){
		        		$(".opacityL").removeClass('hide');
		        	}
		        	else{
		        		$(".opacityL").addClass('hide');
		        	}
		        }
	   	});
	   	
	   	function listToggle(str,obj){
	   		if(str == 'li'){
		   		var box = obj.find('.honor');
				var ico =  obj.find('.icon-triagle-r');
				if(box.hasClass('hide')){
					ico.css('-webkit-transform','rotateZ(90deg)');
					box.removeClass('hide');
				}else{
					ico.css('-webkit-transform','rotateZ(0)');
					box.addClass('hide');
				}
	   		}else{
	   			$alert = $("#alert");
	   			var src = obj.find('img').attr('src');
	   			$alert.find('img').attr('src',src).show();
	   			$alert.show();
	   		}
	   	}
	   	
	   	$("#alert").click(function(){
	   		$(this).hide();
	   	})
	   	
	   	$(".honorWrap").find('li').click(function(){
			listToggle('li',$(this));
			return false;
		})
		$(".imgBox").click(function(){
			listToggle('imgBox',$(this));
			return false;
		})
    });
</script>
